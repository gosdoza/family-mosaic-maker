    "trace:open": "playwright show-trace ./test-results/**/*.zip || echo 'No trace found'",
    "check:api": "./scripts/check-api.sh",
    "health:check": "./scripts/health-check.sh",
    "verify:env": "./scripts/verify-env.sh",
    "setup:db": "./scripts/setup-db.sh",
    "env:guide": "tsx scripts/print-vercel-env-guide.ts",
    "vercel:env:auto": "bash ./scripts/setup-vercel-env-auto.sh",
    "vercel:env:quick": "bash ./scripts/setup-vercel-env-quick.sh",
    "vercel:env:setup": "bash ./scripts/setup-vercel-env.sh",
    "vercel:preview:redeploy": "vercel --prebuilt --prod=false --yes",
    "vercel:prod:redeploy": "vercel --prebuilt --prod --yes",
    "test:auth:nonmock": "cross-env NEXT_PUBLIC_USE_MOCK=false playwright test e2e/auth-protection-nonmock.spec.ts",
    "smoke:signed-download": "node scripts/smoke/signed-download.mjs",
    "retention:cleanup": "node scripts/retention/cleanup.mjs",
    "retention:cleanup:dry-run": "node scripts/retention/cleanup.mjs --dry-run",
    "retention:cleanup:10": "node scripts/retention/cleanup.mjs --percent=10",
